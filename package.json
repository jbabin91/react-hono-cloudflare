{
  "name": "react-hono-cloudflare",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build && vite build --mode page-function",
    "db:generate": "drizzle-kit generate --config ./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./drizzle.config.ts",
    "deploy": "pnpm build && wrangler pages deploy",
    "dev": "vite",
    "format": "prettier -uw --cache .",
    "format:check": "prettier -uc --cache .",
    "lint": "eslint . --max-warnings 0 --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:fix": "eslint . --fix --cache --cache-location node_modules/.cache/.eslintcache",
    "prepare": "husky",
    "preview": "pnpm build && wrangler pages dev",
    "typecheck": "tsc --noEmit"
  },
